@{
}
<h2>Employee List</h2>

<table>
    <tr>

        <th>EmployeeId</th>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Email</th>
        <th>Department</th>
    </tr>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.EmployeeId</td>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>@employee.Email</td>
            <td>@employee.Department</td>
        </tr>
    }
</table>
